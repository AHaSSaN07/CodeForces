#include<iostream>
#include <stdlib.h>
#include <algorithm>

using namespace std;


int main()
{
	long long n, m;
	cin >> n >> m;
	long long power = 1;
	for (int i = 0; i < n; i++)
	{
		power *= 2;
		if (power > m) {  //هخه
			cout << m << endl;
		exit(0);
		}
	}
	
	cout << m%power << endl; 
	return 0;
}
