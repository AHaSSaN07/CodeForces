#include<bits/stdc++.h>
using namespace std;
int main()
{
	int matrix[5][5];
	int ii;
	int jj;
	int counter = 0;
	for (int i = 0; i < 5; i++)
	{
		for (int j = 0; j < 5; j++)
		{
			cin >> matrix[i][j];
			if (matrix[i][j] == 1)
			{
				ii = i;
				jj = j;
			}
		}
	}

	cout << abs(ii - 2) + abs(jj - 2) << endl;
}
