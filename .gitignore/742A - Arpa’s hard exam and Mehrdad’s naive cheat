#include<iostream>
#include <algorithm>

using namespace std;


int main()
{
	int n;
	cin >> n;
	while (n != 0)
	{

		if (n == 1 || n % 4 == 1) {
			cout << 8 << endl;
			break;
		}
		else if (n == 2 || n % 4 == 2)
		{
			cout << 4 << endl;
			break;

		}
		else if (n == 3 || n % 4 == 3)
		{
			cout << 2 << endl;
			break;

		}
		else if (n == 4 || n % 4 == 0)
		{
			cout << 6 << endl;
			break;

		}
	}
	if (n == 0) {
		cout << 1;

	}
}
