#include <bits/stdc++.h>
using namespace std;

int main()
{
	int n;
	cin >>n;
	if (n < 4) { cout << "NO\n"; return 0; }
	cout << "YES\n";
	while (n>5)
	{
		cout << n <<" - "<< n - 1<< " = 1\n";
		n -= 2;
		cout << "1 * 1 = 1\n";
	}
	if (n == 5)
	{
		cout << "4 * 5 = 20\n";
		cout << "2 + 3 = 5\n";
		cout << "5 - 1 = 4\n";
		cout << "20 + 4 = 24\n";
	}
	else
	{
		cout << "1 * 2 = 2\n";
		cout << "2 * 3 = 6\n";
		cout << "6 * 4 = 24\n";
	}	
	return 0;
}
