#include <bits/stdc++.h>
using namespace std;

int main()
{
	string input;
	cin >> input;
	bool flag = false;
	for (int i = 0; i < input.size(); i++)
	{
		if (input[i] == 'W' && input[i + 1] == 'U' && input[i + 2] == 'B'&& (i + 2) < input.size())
		{
			if (flag)cout << " ";
			i += 2;
			continue;
		}
		else
		{
			flag = true;
			cout << input[i];
		}
	}
	//cout << input;

}
	
