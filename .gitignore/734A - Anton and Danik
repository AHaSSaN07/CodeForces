#include<bits/stdc++.h>

using namespace std;


bool desc(int i, int j) { return i > j; }




int main()
{
	int n;
	cin >> n;
	string input;
	cin >> input;
	int as = 0;
	int ds = 0;
	for (int i = 0; i < n; i++)
	{
		if (input[i] == 'A')
		{
			as++;
		}
		else if(input[i] == 'D')
		{
			ds++;
		}
	}
	if (as > ds)cout << "Anton\n";
	if (as < ds)cout << "Danik\n";
	if (as == ds)cout << "Friendship\n";
	
}
